{
  "name": "@fastics/components",
  "version": "0.1.1",
  "scripts": {
    "build:cjs": "babel src -d dist/cjs --env-name cjs --extensions \".ts,.tsx,.js,.jsx\"",
    "build:esm": "babel src -d dist/esm --extensions \".ts,.tsx,.js,.jsx\"",
    "build:js": "yarn build:cjs && yarn build:esm",
    "build:ts:esm": "tsc -p tsconfig.build.json",
    "build:ts:cjs": "tsc -p tsconfig.build.json -module commonjs --outDir dist/cjs",
    "build:ts": "yarn build:ts:esm && yarn build:ts:cjs",
    "build": "npx rimraf dist && yarn build:js && yarn build:ts",
    "test": "TZ=UTC react-scripts test",
    "lint": "eslint .",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "prepare": "husky install",
    "pre-commit": "lint-staged",
    "release": "semantic-release"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "files": ["/dist"],
  "peerDependencies": {
    "react": ">=16",
    "react-dom": ">=15",
    "styled-components": "^5.3.0"
  },
  "dependencies": {
    "classnames": "^2.3.1",
    "core-js": "^3.16.1",
    "react-jss": "^10.7.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.15.0",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.15.0",
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@dbenfouzari/cli": "^5.1.1",
    "@semantic-release/npm": "^7.1.3",
    "@storybook/addon-actions": "^6.3.6",
    "@storybook/addon-essentials": "^6.3.6",
    "@storybook/addon-links": "^6.3.6",
    "@storybook/node-logger": "^6.3.6",
    "@storybook/preset-create-react-app": "^3.2.0",
    "@storybook/react": "^6.3.6",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^12.0.0",
    "@testing-library/react-hooks": "^7.0.1",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "^5.1.7",
    "@types/react-window": "^1.8.4",
    "@types/styled-components": "^5.1.11",
    "babel-plugin-transform-scss": "^1.0.11",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "husky": "^7.0.0",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "react-spring": "^9.2.4",
    "react-window": "^1.8.6",
    "sass": "^1.37.5",
    "semantic-release": "^17.4.4",
    "styled-components": "^5.3.0",
    "typescript": "^4.1.2"
  },
  "resolutions": {
    "babel-loader": "8.1.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ],
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md,mdx}": [
      "prettier --write"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "!src/index.tsx",
      "!src/logo.svg",
      "!src/reportWebVitals.ts",
      "!src/**/*.stories.tsx",
      "!src/stories/*.*"
    ]
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "tag": "latest",
    "access": "public"
  }
}
